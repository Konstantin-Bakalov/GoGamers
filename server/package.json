{
    "name": "games-library",
    "version": "1.0.0",
    "description": "an online library for games",
    "main": "index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "nodemon",
        "tsc": "tsc -p .",
        "knex": "knex --knexfile knexfile.ts",

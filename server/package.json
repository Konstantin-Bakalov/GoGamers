        "url": "https://github.com/Konstantin-Bakalov/Games-Library/issues"
    },
    "homepage": "https://github.com/Konstantin-Bakalov/Games-Library#readme",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/convict": "^6.1.1",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^18.6.5",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
